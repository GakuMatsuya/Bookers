<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
    </tr>
  </thead>
  
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.title%></td>
      </tr>
      <tr>
        <td><%= book.body %></td>
      </tr>
      <td>show</td>
      <td>edit</td>
      <td>destroy</td>
    <% end %>  
  </tbody>
  
</table>
